{
    "name": "lizundao/k3cloud-webapi-sdk",
    "description": "金蝶K3Cloud WebAPI SDK for PHP - 提供金蝶K3Cloud系统的WebAPI接口调用（根据 金蝶PHP SDK V8.2.0 开发）",
    "type": "library",
    "keywords": ["kingdee", "k3cloud", "webapi", "sdk", "erp", "php"],
    "license": "MIT",
    "homepage": "https://github.com/lizundao/k3cloud-webapi-sdk",
    "support": {
        "issues": "https://github.com/lizundao/k3cloud-webapi-sdk/issues",
        "source": "https://github.com/lizundao/k3cloud-webapi-sdk"
    },
    "authors": [
        {
            "name": "lizundao",
            "email": "lizundao@qq.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^8.0|^9.0",
        "phpstan/phpstan": "^0.12",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Lizundao\\K3CloudSDK\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lizundao\\K3CloudSDK\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage",
        "cs-check": "phpcs",
        "cs-fix": "phpcbf"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
